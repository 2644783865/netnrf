<!DOCTYPE html>
<html>
<head>

    <meta http-equiv="X-UA-Compatible" content="chrome=1,IE=edge" />
    <meta charset="utf-8" />
    <link rel="icon" href="/favicon.ico" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <title>@ViewData["Title"] netnrf响应式框架</title>

    <!-- Bootstrap、字体 -->
    <link href="https://lib.baomitu.com/twitter-bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://lib.baomitu.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">

    <!-- jQuery、Bootstrap -->
    <script src="https://lib.baomitu.com/jquery/1.12.4/jquery.min.js"></script>
    <script src="https://lib.baomitu.com/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>

    <!-- EasyUI -->
    <link href="/lib/jquery-easyui/themes/default/easyui.css" rel="stylesheet" />
    <script src="/lib/jquery-easyui/jquery.easyui.min.min.js"></script>

    <!--页面自定义样式||重写-->
    <link href="/css/z.css" rel="stylesheet" />
    <script src="/js/z.js"></script>
</head>
<body>
    <div class="container-fluid">
        @{
            string current_url = "/" + ViewContext.RouteData.Values["controller"]?.ToString() + "/" + ViewContext.RouteData.Values["action"]?.ToString().ToLower();
            Html.RenderAction("Button", "Inlay", new { current_url });
        }
        <div id="myBody">
            @RenderBody()
        </div>
    </div>
</body>
</html>