@{
    ViewBag.Title = "Two";
}

<form class="form-horizontal formui" id="_af_form_1">
    <div class="row">

        <ul class="nav nav-tabs" role="tablist" id="_af_tablist_1">
            <li class='active'>
                <a href="#_afpp_1" data-toggle="tab">面板 1</a>
            </li>
            <li class=''>
                <a href="#_afpp_2" data-toggle="tab">面板 2</a>
            </li>
        </ul>
        <div class="tab-content" id="_aftc_1">
            <div class='tab-pane active' id="_afpp_1">
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label requiredTag'>虚拟表名</label>
                        <div>
                            <input type='text' id="vname" name="vname" class="form-control" value="" data-type="" data-url="" placeholder="请输入虚拟表名" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label '>主键ID</label>
                        <div>
                            <input type='text' id="id" name="id" class="form-control" value="" data-type="" data-url="" placeholder="自增列，不填写" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label requiredTag'>列字段</label>
                        <div>
                            <input type='text' id="field" name="field" class="form-control" value="" data-type="" data-url="" placeholder="请输入列字段" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label requiredTag'>显示标题</label>
                        <div>
                            <input type='text' id="title" name="title" class="form-control" value="" data-type="" data-url="" placeholder="请输入显示标题" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label '>显示列宽</label>
                        <div>
                            <input type='text' id="width" name="width" class="form-control" value="" data-type="" data-url="" placeholder="请输入显示列宽" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label '>对齐方式</label>
                        <div>
                            <input type='text' id="align" name="align" class="form-control" value="" data-type="combobox" data-url="ls_align" placeholder="" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label requiredTag'>是否隐藏</label>
                        <div>
                            <input type='text' id="hide" name="hide" class="form-control" value="" data-type="combobox" data-url="ls_hide" placeholder="" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
