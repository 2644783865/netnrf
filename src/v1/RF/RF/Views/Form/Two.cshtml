                    <div class="form-group">
                        <label class='control-label '>表单类型</label>
                        <div>
                            <input type='text' id="f_type" name="f_type" class="form-control" value="" data-type="combobox" data-url="ls_f_type" placeholder="" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label '>所在区域</label>
                        <div>
                            <input type='text' id="f_area" name="f_area" class="form-control" value="" data-type="combobox" data-url="ls_f_area" placeholder="" />
                        </div>
                    </div>
                </div>
                <div class='col-md-12 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label '>URL</label>
                        <div>
                            <input type='text' id="f_url" name="f_url" class="form-control" value="" data-type="" data-url="" placeholder="" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label '>是否必填</label>
                        <div class='checkbox'>
                            <label>
                                <input type="checkbox" value="1" id="f_required" name="f_required" data-type="checkbox" data-url="" placeholder="" />
