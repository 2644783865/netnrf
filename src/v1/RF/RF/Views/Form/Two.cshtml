                        <label class='control-label '>显示排序</label>
                        <div>
                            <input type='text' id="l_order" name="l_order" class="form-control" value="" data-type="" data-url="" placeholder="" />
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label '>冻结列</label>
                        <div class='checkbox'>
                            <label>
                                <input type="checkbox" value="1" id="frozen" name="frozen" data-type="checkbox" data-url="" placeholder="" />
                                是冻结列
                            </label>
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label '>启用排序</label>
                        <div class='checkbox'>
                            <label>
                                <input type="checkbox" value="1" id="sort" name="sort" data-type="checkbox" data-url="" placeholder="" />
                                点击标题排序
                            </label>
                        </div>
                    </div>
                </div>
                <div class='col-md-6 col-sm-6 '>
                    <div class="form-group">
                        <label class='control-label '>默认标题</label>
